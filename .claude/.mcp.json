{
  "mcpServers": {
    "notionApi": {
      "command": "npx",
      "args": ["-y", "@notionhq/notion-mcp-server"],
      "env": {
        "NOTION_TOKEN": "${NOTION_TOKEN}"
      }
    },
    "mcp-gsuite": {
      "command": "uv",
      "args": [
        "--directory",
        "/Users/edmund/MyAssistant/gsuite-mcp",
        "run",
        "mcp-gsuite-enhanced"
      ],
      "env": {
        "PYTHONPATH": "/Users/edmund/MyAssistant/gsuite-mcp",
        "UV_PROJECT_ENVIRONMENT": "/Users/edmund/MyAssistant/gsuite-mcp/.venv"
      }
    }
  }
}
